RewriteEngine on
RewriteEngine on

# Ce fichier de configuration doit être téléchargé sur le serveur sous "<Dossier de l'application>/Build/.htaccess"
# NOTE : Le module Apache "mod_mime" doit être activé pour que cette configuration fonctionne.
<IfModule mod_mime.c>

# Les lignes suivantes sont nécessaires pour les constructions sans repli de décompression, compressées avec gzip
RemoveType .gz
AddEncoding gzip .gz
AddType application/octet-stream .data.gz # Utilisez le type MIME application/octet-stream au lieu de application/gzip en raison d'un bug Safari https://bugs.webkit.org/show_bug.cgi?id=247421.
AddType application/wasm .wasm.gz
AddType application/javascript .js.gz
AddType application/octet-stream .symbols.json.gz

# Les lignes suivantes sont nécessaires pour les constructions sans repli de décompression, compressées avec Brotli
RemoveType .br
RemoveLanguage .br
AddEncoding br .br
AddType application/octet-stream .data.br
AddType application/wasm .wasm.br
AddType application/javascript .js.br
AddType application/octet-stream .symbols.json.br

# La ligne suivante améliore la performance de chargement pour les constructions non compressées
AddType application/wasm .wasm

# Décommentez la ligne suivante pour améliorer la performance de chargement pour les constructions compressées avec gzip ayant un repli de décompression
# AddEncoding gzip .unityweb

# Décommentez la ligne suivante pour améliorer la performance de chargement pour les constructions compressées avec Brotli ayant un repli de décompression
# AddEncoding br .unityweb

</IfModule>

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(.*)$ index.php?url=$1 [QSA,L]
